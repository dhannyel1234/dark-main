# OpenPix Configuration
# Copie este arquivo para .env.local e configure com seus valores reais

# IMPORTANTE: Para pagamentos Pix, você precisa do AppID da OpenPix
# O AppID é um ClientID:ClientSecret codificado em Base64
# Exemplo: Client_Id_123456:Client_Secret_abcdef -> Q2xpZW50X0lkXzEyMzQ1NjpDbGllbnRfU2VjcmV0X2FiY2RlZg==
OPENPIX_APP_ID="seu-app-id-aqui"

# EFI Payment Configuration
# Copie este arquivo para .env.local e configure com seus valores reais

# Configurações EFI
EFI_CLIENT_ID="seu-client-id"
EFI_CLIENT_SECRET="seu-client-secret"
EFI_CERT_PATH="caminho/para/seu/certificado.p12"
EFI_PIX_KEY="sua-chave-pix"


# Discord OAuth
DISCORD_CLIENT_ID="your-discord-client-id"
DISCORD_CLIENT_SECRET="your-discord-client-secret"

# Database
DATABASE_URL="postgresql://postgres:your-password@db.xbqztnrroqqgunukqlmf.supabase.co:5432/postgres"

# Supabase
NEXT_PUBLIC_SUPABASE_URL="your-supabase-url"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-supabase-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-supabase-service-role-key"

# Azure Configuration
AZURE_TENANT_ID="your-azure-tenant-id"
AZURE_CLIENT_ID="your-azure-client-id"
AZURE_CLIENT_SECRET="your-azure-client-secret"
AZURE_SUBSCRIPTION_ID="your-azure-subscription-id"
AZURE_RESOURCE_GROUP_NAME="your-azure-resource-group"
AZURE_DEFAULT_SNAPSHOT_ID="/subscriptions/xxx/resourceGroups/xxx/providers/Microsoft.Compute/snapshots/your-snapshot"
AZURE_VNET_RESOURCE_GROUP="your-vnet-resource-group"
AZURE_VNET_NAME="your-vnet-name"
AZURE_SUBNET_NAME="your-subnet-name"

# Azure VM Credentials
AZURE_MACHINE_USERNAME="your-vm-username"
AZURE_MACHINE_PASSWORD="your-vm-password"